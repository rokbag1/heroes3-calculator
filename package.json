{
    "name": "heroes-3-calculator",
    "description": "Heroes 3 Calculator",
    "private": true,
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "test": "tsc -p . --noEmit && eslint \"src/**/*.ts*\"",
        "watch": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
        "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
        "serve:prod": "npm run build:prod && cross-env NODE_ENV=production webpack-dev-server --config webpack.config.js",
        "start": "npm run watch"
    },
    "license": "MIT",
    "devDependencies": {
        "@reactway/eslint-config": "1.0.0-alpha",
        "@reactway/webpack-builder": "2.0.0",
        "@reactway/webpack-builder-plugin-clean": "2.0.0",
        "@reactway/webpack-builder-plugin-html": "2.0.0",
        "@reactway/webpack-builder-plugin-images": "2.0.0",
        "@reactway/webpack-builder-plugin-styles": "2.0.0",
        "@reactway/webpack-builder-plugin-typescript": "2.0.0",
        "@reactway/webpack-builder-plugin-web-dev": "2.0.0",
        "@reactway/webpack-builder-plugin-write-file": "2.0.0",
        "@types/react": "16.9.4",
        "@types/react-dom": "16.9.1",
        "@types/react-router-dom": "^5.1.3",
        "cross-env": "6.0.2",
        "eslint": "6.5.1",
        "node-sass": "4.12.0",
        "typescript": "3.7.3",
        "webpack": "4.41.0",
        "webpack-cli": "3.3.9",
        "webpack-dev-server": "3.8.1"
    },
    "dependencies": {
        "@babel/polyfill": "7.6.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.27",
        "@fortawesome/free-solid-svg-icons": "^5.12.1",
        "@fortawesome/react-fontawesome": "^0.1.9",
        "@types/history": "^4.7.3",
        "react": "16.8.6",
        "react-dom": "16.8.6",
        "react-fontawesome": "^1.7.1",
        "react-router-dom": "^5.1.2",
        "reactjs-popup": "^1.5.0"
    }
}
